<div class="text-center">
  <h1>New Season</h1>
</div>

<%= form_for @season, html: { class: "form-horizontal" } do |f| %>

  <%= render "season_fields", f: f %>

  <div class="row">
    <div class="col-md-10 col-md-offset-2">
      <%= f.submit "Create", class: "btn btn-primary" %>
    </div>
  </div>

<% end %>
